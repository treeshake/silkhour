{
  "compilerOptions": {
    "outDir": "./assets/custom",
    "noImplicitAny": true,
    "esModuleInterop" : true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  }
}
